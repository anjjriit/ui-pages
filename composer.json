{
    "name": "shawnsandy/pagekit",
    "type": "library",
    "description": "Laravel Ui.Pages",
    "keywords": [
        "shawnsandy",
        "PageKit"
    ],
    "homepage": "https://github.com/shawnsandy/ui-pages",
    "license": "MIT",
    "authors": [{
        "name": "Shawn Sandy",
        "email": "shawnsandy04@gmail.com",
        "homepage": "http://shawnsandy.design",
        "role": "Developer"
    }],
    "require": {
        "php": "~5.6|~7.0",
        "michelf/php-markdown": "^1.6",
        "illuminate/contracts": "~5.3|~5.4",
        "illuminate/http": "~5.3|~5.4",
        "illuminate/support": "~5.3|~5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ShawnSandy\\PageKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShawnSandy\\PageKit\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ShawnSandy\\PageKit\\PageKitServiceProvider"
            ],
            "aliases": {
                "Pages": "ShawnSandy\\PageKit\\Facades\\PageFacade"
            }
        },
        "config": {
            "sort-packages": true
        }


    },
    "config": {
        "sort-packages": true
    }

}
